{
  "name": "frontend",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Quasar App",
  "author": "Gil Engel <fernwaerts@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ui": "majestic",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "pretest": "rm -rf .nyc_output || true",
    "posttest": "npm run report:combined",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:8080 \"cypress open --e2e\"",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "npm run mkdir:reports",
    "copy:reports": "cp test/cypress/coverage/coverage-final.json reports/from-cypress.json && cp test/jest/coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "npm run copy:reports && mkdir .nyc_output || true",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "npm run combine:reports",
    "report:combined": "npx nyc report --reporter lcov --reporter text --report-dir coverage",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:8080 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@pinia/testing": "^0.1.3",
    "@quasar/extras": "^1.16.11",
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.8",
    "jest-mock-axios": "^4.7.3",
    "line-awesome": "^1.3.0",
    "pinia": "^2.1.7",
    "quasar": "^2.16.4",
    "sortablejs": "^1.15.2",
    "sortablejs-vue3": "^1.2.11",
    "ts-mock-imports": "^1.3.15",
    "uuid": "^9.0.1",
    "vue": "^3.4.27",
    "vue-draggable-next": "^2.2.1",
    "vue-router": "^4.3.2",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@quasar/app-vite": "^1.9.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.1.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.3",
    "@types/node": "^20.12.12",
    "@types/string-similarity": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.9.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "majestic": "^1.8.1",
    "prettier": "^3.2.5",
    "rete": "2.0.3",
    "rete-connection-plugin": "2.0.2",
    "rete-minimap-plugin": "2.0.2",
    "rete-vue-plugin": "2.1.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": "^20",
    "npm": ">= 10.1.0",
    "yarn": ">= 1.22.19"
  }
}
