    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "name": "Page Frontend FIREFOX",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/frontend/src",
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
              "webpack:///./src/*": "${webRoot}/*"
            }
          }
          
    ]
}